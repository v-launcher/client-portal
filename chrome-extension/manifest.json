{
  "manifest_version": 3,
  "name": "Client Portal Extension",
  "version": "1.0.0",
  "description": "Chrome extension that listens to external messages for the Client Portal PWA",
  "permissions": [],
  "update_url": "https://v-launcher.github.io/client-portal/chrome-extension/update.xml",
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": [
      "https://v-launcher.github.io/*",
      "http://localhost:*",
      "https://localhost:*"
    ]
  },
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  }
}